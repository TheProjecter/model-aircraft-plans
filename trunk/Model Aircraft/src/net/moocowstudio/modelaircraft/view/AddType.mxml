                  y="10"
                  text="Add new model aircraft type"
                  fontSize="13"
                  fontWeight="bold"
                  horizontalCenter="0" />
        <mx:TextInput id="txtTitle"
                      x="55"
                      y="45"
                      width="181" />
        <mx:Button label="+"
                   click="{this.onClick_btnAdd(event)}"
                   x="196"
                   y="75" />
