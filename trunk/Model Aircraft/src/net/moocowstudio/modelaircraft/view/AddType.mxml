              height="300"
              width="525"
              borderStyle="none">
    <mx:Metadata>
        [Event(name="save", type="flash.events.Event")]
    </mx:Metadata>
    <mx:Canvas y="28"
               width="248"
               height="109"
               horizontalCenter="0"
               backgroundAlpha="0.4"
               backgroundColor="#FFFFFF"
